<?xml version="1.0"?>
<robot name="pantry">
  <link name="pantry">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="30.0"/>
        <inertia ixx="4e-2" ixy="0" ixz="0" iyy="4e-2" iyz="0" izz="4e-2"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="pantry_visual.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="pantry_collision.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.01"/>
      </surface>
    </collision>
  </link>
</robot>

