<?xml version="1.0"?>
<robot name="doughclump">
  <link name="doughclump">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.001"/>
        <inertia ixx="3.6e-6" ixy="0" ixz="0" iyy="3.6e-6" iyz="0" izz="3.6e-6"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="./cookie.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="./cookie.obj"/>
      </geometry>
    </collision>
  </link>
  <link name="sprinkle">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.001"/>
        <inertia ixx="3.6e-7" ixy="0" ixz="0" iyy="3.6e-7" iyz="0" izz="3.6e-7"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="./sugarsprinkle.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="./sugarsprinkle.obj"/>
      </geometry>
    </collision>
  </link>
  <joint name="sprinkle" type="fixed">
    <parent link="doughclump"/>
    <child link="sprinkle"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
</robot>

