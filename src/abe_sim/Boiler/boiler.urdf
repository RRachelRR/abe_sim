<?xml version="1.0"?>
<robot name="boiler">
  <link name="boiler">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="30.0"/>
        <inertia ixx="2e-2" ixy="0" ixz="0" iyy="2e-2" iyz="0" izz="2e-2"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="boiler_visual.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="boiler_visual.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
</robot>

