<?xml version="1.0"?>
<robot name="kitchenStoveDoor">
  <link name="world"/>
  <link name="kitchenstove_door">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="2.0"/>
        <inertia ixx="0.013" ixy="0" ixz="0" iyy="0.419" iyz="0" izz="0.406"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="kitchenstove_door_visual.obj"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="kitchenstove_door_collision.obj"/>
      </geometry>
    </collision>
  </link>
  <joint name="kitchenstove_hinge" type="revolute">
    <axis xyz="1 0 0"/>
    <parent link="world"/>
    <child link="kitchenstove_door"/>
    <limit effort="1000.0" lower="-1.1" upper="0" velocity="3."/>
    <origin rpy="0 0 0" xyz="0 0.653 -0.428"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
</robot>

