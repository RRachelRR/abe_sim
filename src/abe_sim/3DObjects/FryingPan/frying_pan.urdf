<?xml version="1.0"?>
<robot name="frying_pan">
  <link name="pan">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.01"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="fryingPan.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="fryingPanCollider.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
  <link name="grip">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.01"/>
        <inertia ixx="1.9e-5" ixy="0" ixz="0" iyy="6.8e-6" iyz="0" izz="1.2e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="fryingPanGrip.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="fryingPanGrip.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
  <joint name="pan_to_grip" type="fixed">
    <parent link="pan"/>
    <child link="grip"/>
    <origin rpy="0 0 0" xyz="0.27671 0 0.044774"/>
  </joint>
</robot>

