<?xml version="1.0"?>
<robot name="bacon">
  <link name="body">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.03"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="CookedBacon.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="baconCollider.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
<link name="stripe1">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.03"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="BaconStripe.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="stripeCollider.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
<link name="stripe2">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.03"/>
        <inertia ixx="4.4e-5" ixy="0" ixz="0" iyy="0" iyz="0" izz="4.4e-5"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="BaconStripe.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="stripeCollider.obj"/>
      </geometry>
      <surface>
           <bounce restitution_coefficient = "0.1"/>
      </surface>
    </collision>
  </link>
  <joint name="body_to_stripe1" type="fixed">
    <parent link="body"/>
    <child link="stripe1"/>
    <origin rpy="0 0 0" xyz="-0.02895 0.014182 0"/>
  </joint>
  <joint name="body_to_stripe2" type="fixed">
    <parent link="body"/>
    <child link="stripe2"/>
    <origin rpy="0 0 0" xyz="0.03135 0.014182 0"/>
  </joint>
</robot>

