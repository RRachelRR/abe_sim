<?xml version="1.0"?>
<robot name="spoon">
  <link name="spoon">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.1"/>
        <inertia ixx="3.6e-6" ixy="0" ixz="0" iyy="3.6e-6" iyz="0" izz="3.6e-6"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="spoon_visual.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="spoon_collision.obj"/>
      </geometry>
    </collision>
  </link>
  <link name="spoon_a">
    <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.1"/>
        <inertia ixx="3.6e-6" ixy="0" ixz="0" iyy="3.6e-6" iyz="0" izz="3.6e-6"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="spoon_visual.obj"/>
      </geometry>
    </visual>
  </link>
  <joint name="brr" type="continuous">
    <axis xyz="1 0 0"/>
    <parent link="spoon"/>
    <child link="spoon_a"/>
    <limit effort="1000.0" velocity="3."/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
</robot>

